{
  "name": "hubot-privileges",
  "description": "Sometimes hubot gets abused. Stop the abuse by taking hubot away from them",
  "version": "1.0.4",
  "author": "ndaversa",
  "license": "MIT",
  "keywords": [
    "hubot",
    "hubot-scripts"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ndaversa/hubot-privilege.git"
  },
  "bugs": {
    "url": "https://github.com/ndaversa/hubot-privilege/issues"
  },
  "dependencies": {
    "hubot": ">=2.3.0",
    "hubot-scripts": "*"
  },
  "devDependencies": {
    "blanket": "~1.1.6",
    "chai": "*",
    "coffee-script": "^1.10.0",
    "coveralls": "~2.5.0",
    "mocha": "*",
    "mocha-lcov-reporter": "0.0.1",
    "sinon": "*",
    "sinon-chai": "*"
  },
  "main": "index.coffee",
  "scripts": {
    "test": "make test"
  },
  "config": {
    "blanket": {
      "pattern": "hubot-privilege/src",
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": "node_modules"
    }
  }
}
